<?xml version="1.0" encoding="UTF-8"?>
<model package="multimedialibrary" baseClass="xPDOObject" platform="mysql" defaultEngine="MyISAM" version="1.1" phpdoc-package="multimedialibrary" phpdoc-subpackage="model">
